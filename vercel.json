{
  "version": 2,
  "name": "dota2-teammate-finder",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://dota2comfyback2.onrender.com/api",
    "VITE_WS_URL": "https://dota2comfyback2.onrender.com"
  }
}